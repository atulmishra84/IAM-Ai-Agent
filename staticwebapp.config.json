{
  "routes": [
    {
      "route": "/admin",
      "rewrite": "/admin/index.html"
    },
    {
      "route": "/admin/",
      "rewrite": "/admin/index.html"
    },
    {
      "route": "/api/*"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/admin/*", "/api/*"]
  }
}
