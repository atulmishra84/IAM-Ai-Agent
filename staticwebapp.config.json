{
  "routes": [
    {
      "route": "/admin",
      "rewrite": "/admin/index.html"
    },
    {
      "route": "/admin/",
      "rewrite": "/admin/index.html"
    },
    {
      "route": "/demo",
      "rewrite": "/demo/interactive-demo.html"
    },
    {
      "route": "/demo/",
      "rewrite": "/demo/interactive-demo.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/admin/*", "/api/*", "/demo/*"]
  }
}
