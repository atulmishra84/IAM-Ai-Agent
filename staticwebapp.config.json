{
    "routes": [
        {
            "route": "/admin/",
            "rewrite": "/admin/index.html"
        },
        {
            "route": "/admin/*",
            "rewrite": "/admin/index.html"
        },
        {
            "route": "/admin",
            "rewrite": "/admin/index.html"
        },
        {
            "route": "/api/*"
        }
    ],
    "navigationFallback": {
        "rewrite": "/index.html"
    },
    "globalHeaders": {
        "X-Content-Type-Options": "nosniff",
        "X-Frame-Options": "DENY",
        "Content-Security-Policy": "default-src 'self' https: data: 'unsafe-inline' 'unsafe-eval'"
    },
    "mimeTypes": {
        ".json": "application/json",
        ".js": "application/javascript",
        ".css": "text/css"
    }
}
